<launch>

  <arg name="gzname" default="gazebo"/>
  <arg name="gzworld" default="empty.world"/>
  <param name="/use_sim_time" type="bool" value="true"/>

  <!-- start gazebo -->
  <node name="gazebo" pkg="pal_gazebo_plugins" type="run_$(arg gzname)" args="$(find reem_gazebo)/worlds/simple_office.world" output="screen" />

  <!-- start REEM robot -->
  <include file="$(find reem_gazebo)/launch/reem_pal.launch"/>

</launch>
